
<!DOCTYPE html>
<html>

<% include ../partials/stylesheetLinks %>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.7.3/socket.io.js"></script>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">


  <% include ../partials/header %>

  <% include ../partials/navShip %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->

    <section class="content-header">
                <h1>
        <%= result[0].FName %> <%= result[0].LName %>
        <small>profile details</small>
    </h1>

        </section>

        <!-- Main content -->
        <% if (message1.length > 0) { %>
          <div class="alert alert-danger"><%= message1 %></div>
        <% } %>
        <section class="content">
<div class="nav-tabs-custom">
    <!-- Tab panes -->
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="details">
            <div class="row">
                <div class="col-lg-8 col-md-7">
                    <div class="panel panel-default">
    <div class="panel-heading">Change User Details</div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-10">

                <form action="/editprofileShippingAdmin" method="post">
                <div class="form-group">
                    <label for="first_name">Username   :</label>
                  <input  type="text" id="username" name="username" value= "<%= result[0].Username %>">
                </div>
                <div class="form-group">
                    <label for="first_name">First Name  :</label>
                      <input  type="text" id="fname" name="fname" value= "<%= result[0].FName %>">

                </div>
                <div class="form-group">
                    <label for="last_name">Last Name  :</label>
                      <input  type="text" id="lname" name="lname" value= "<%= result[0].LName %>">

                </div>
                <div class="form-group">
                    <label for="phone">Email       :</label>
                      <input  type="email" id="email" name="email" size="30" value= "<%= result[0].Email %>">

                </div>
                <div class="form-group">
                    <label for="address">Address     :</label>
                      <input  type="text" id="address" name="address" size="30" value= "<%= result[0].Address %>">
                </div>

            <button style="margin-left:20%;" type="submit">Save changes</button>
          </form>
            </div>


    </div>
    </div>

</div>
                </div>

            </div>
        </div>

    </div>
</div>
        </section><!-- /.content -->

        <% if (message.length > 0) { %>
          <div class="alert alert-danger"><%= message %></div>
        <% } %>

        <section class="content">
<div class="nav-tabs-custom">


    <!-- Tab panes -->
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="details">
            <div class="row">
                <div class="col-lg-8 col-md-7">
    <div class="panel panel-default">
    <div class="panel-heading">Change Password</div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-10">


              <script type="text/javascript">
                        function validateForm(){
                            if((document.forms["passwordChange"]["Curpassword"].value == "")|| (document.forms["passwordChange"]["password"].value == "")|| (document.forms["passwordChange"]["Cpassword"].value == ""))  {
                            alert("Password fields must be filled out");
                            return false;
                            }
                            else if(document.forms["passwordChange"]["password"].value.length<7)  {
                            alert("Please enter more than 6 characters for New Password");
                            return false;
                            }
                            else if(document.forms["passwordChange"]["password"].value != document.forms["passwordChange"]["Cpassword"].value)  {
                            alert("New Passwords does not match");
                            return false;
                            }
                        }
                    </script>


                  <form name="passwordChange" action="/editpasswordShippingAdmin" method="post" onsubmit="return validateForm()">
                <div class="form-group">
                    <label for="first_name">Username   :</label>
                  <label> <%= result[0].Username %></label>
                </div>
                <div class="form-group">
                    <label for="address">Current Password   :</label>
                      <input  type="password" id="Curpassword" name="Curpassword" >
                </div>
                <div class="form-group">
                    <label for="address">New Password   :</label>
                      <input  type="password" id="password" name="password" >
                </div>
                <div class="form-group">
                    <label for="address">Confirm New Password   :</label>
                      <input  type="password" id="Cpassword" name="Cpassword" >
                </div>
                      <button style="margin-left:20%;">Save changes</button>
                    </form>
            </div>


    </div>
    </div>

</div>
                </div>

            </div>
        </div>

    </div>
</div>
        </section>


  </div>

</div>
<!-- ./wrapper -->



<% include ../partials/scripts %>


</body>
</html>
